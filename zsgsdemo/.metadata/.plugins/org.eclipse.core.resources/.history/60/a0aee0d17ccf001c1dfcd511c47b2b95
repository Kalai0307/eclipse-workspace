import java.util.ArrayList;

public class EmployeeDetailaArray {

	public static void main(String[] args) {
		ArrayList<Object> e=new ArrayList<Object>();
		System.out.println("employee details");
		e.add("kalai");
		e.add(25);
		e.add(9488034538l);
		for(Object i:e)
		{
			System.out.println(i);
		}
		
		
	
		

	}

}
