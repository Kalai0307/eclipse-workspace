package VidhinSanAssign;

public class Mock {

	public static void main(String[] args) {
		
		char c='a';
		int n=5;
		for(int i=0;i<5;i++)
		{
			for(int j=i;j<n;j++)
			{
				System.out.print(" ");
			}
			for(int k=0;k<=i;k++)
			{
				System.out.print((char)65+k);
			}
			System.out.println();
		}

	}

}
