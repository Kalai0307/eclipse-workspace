package VidhinSanAssign;

public class Pattern4 {

	public static void main(String[] args) {
		int temp=1;
		for(int i=1;i<=5;i++)
		{
			for(int j=i;j<=5;j++)
			{
				System.out.print(" ");
			}
			for(int k=1;k<=i-1;k++)
			{
				System.out.print(k+" ");
				k--;
			}
			System.out.println();
		}

	}

}
