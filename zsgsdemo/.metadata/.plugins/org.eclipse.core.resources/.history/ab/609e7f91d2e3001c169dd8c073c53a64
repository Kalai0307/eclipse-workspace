package VidhinSanAssign;

public class Pattern9 {

	public static void main(String[] args) {int num=1;

	int n=7;
	int temp=0;
	for(int i=1;i<n;i++)
	{
		for(int j=1;j<=i;j++) {
			if(j%2!=0)
			{
				System.out.print(num+num-i);
				 temp=num+num-i;
			}
			else
				System.out.println(temp+2);
		}
		System.out.println();
	}
	}

}
