package com.zoho.busreservation;

public class SeatReservation {

	public static void main(String[] args) {
		System.out.println(" BUS SEAT RESERVATION");
		 int p= '*';
		for(int i=1;i<=42;i++)
		{
			for(int j=1;j<=6;j++)
			{
				if(p%4==0)
				{
					System.out.print(" ");
				}
				else
				{
					System.out.print(p);
				}
					
			}
			System.out.println();
		}

	}

}
