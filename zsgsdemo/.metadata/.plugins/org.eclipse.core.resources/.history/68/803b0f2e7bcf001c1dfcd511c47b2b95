import java.util.ArrayList;
class Employee
{
	int age;
	String name;
	long phno;
	

public Employee(int age,String name,long phno)
{
	this.age=age;
	this.name=name;
	this.phno=phno;
}
}

public class EmployeeDetailaArray {

	public static <Employee> void main(String[] args) {
		ArrayList<Employee> e=new ArrayList<Employee>();
		e.add(new Employee(35,"kalai",948883));
	
		

	}

}
