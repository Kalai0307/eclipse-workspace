package VidhinSanAssign;
// hollow dimaond pattern
public class Pattern2 {

	public static void main(String[] args) {
	int n =5,j;
		for(int i=0;i<n*2;i++)
		{
			for( j=0;j<n*2-i;j++)
			{
				System.out.print("`");
				if(j==n-i||j==n*2-i)
				{
					System.out.print("*");
				}
				else
					System.out.print("=");
			}
			
			System.out.println();
		}
		
	}

}
