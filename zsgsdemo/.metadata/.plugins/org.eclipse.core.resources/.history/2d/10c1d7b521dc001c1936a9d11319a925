package VidhinSanAssign;
/*
 *      *
 *    *   *
 *  *   *   *  
 *    *   *
 *      *
 * 
 * 
 * 
 */
public class Pattern1 {

	public static void main(String[] args) {
		int n=5;
		for(int i=0;i<5;i++)
		{
			for(int j=i;j<n;i++)
			{
				System.out.print("`");
			}
			for(int k=0;k<=i;k++)
			{
				System.out.print("*");
			}
			System.out.println();
		}
		

	}

}
