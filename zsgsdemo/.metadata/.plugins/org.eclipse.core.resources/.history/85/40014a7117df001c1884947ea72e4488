		        {
		          count++;
		          d.add(count);
		          
		        }
		        
		      }if(maxcount<count)
		        {
		        	maxcount=count;
		        }
		       
		        count=0;
		    }
	      System.out.println("max"+maxcount);
	     
		     for(int i:d)
		     {
		    	 if(d.get(i)<maxcount)
		    		 removalcount++;
		     }
	      int result= d.size()-removalcount-1;
	      System.out.println(result);
		        
