package com.zoho.busreservation;

public class SeatReservation {

	public static void main(String[] args) {
		System.out.println(" BUS SEAT RESERVATION");
		System.out.println("PLEASE CHOOSE ONE SEAT");
		int n=5;
		 int p= 1;
		for(int i=1;i<=6;i++)
		{
			for(int j=1;j<=6;j++)
			{
				if(j%4==0)
				{
					System.out.print(" ");
				}
				else 
				{
					System.out.print(p);
				}
				p++;
					
			}
			System.out.println();
		}

	}

}
