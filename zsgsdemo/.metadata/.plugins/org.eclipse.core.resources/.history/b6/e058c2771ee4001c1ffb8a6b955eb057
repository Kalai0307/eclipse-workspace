package VidhinSanAssign;

public class Pattern9 {

	public static void main(String[] args) {

	int n=7;
	int temp=0;
	for(int i=1;i<=n;i++)
	{int num=i;
		for(int j=1;j<=i;j++) {
			if(j%2!=0)
			{
				System.out.print(num+num-i);
				temp=num+num-i;
				 System.out.print("temp"+temp);
				 temp=0;
				 
			}
			
			else
				System.out.print();
		}
		System.out.println();
		num++;
	}
	}

}
