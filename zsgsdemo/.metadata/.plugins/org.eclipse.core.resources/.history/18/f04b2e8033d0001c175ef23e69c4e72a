import java.util.Scanner;

public class WeightProgram {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		System.out.println("number of element");
		int n = sc.nextInt();
		int a[]=new int [n];
		for(int i=0;i<n;i++)
			{
			a[i]=sc.nextInt();	
			}
		System.out.println(a);

	}

}
