package VidhinSanAssign;

public class Pattern3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
int n=5;
for(int i=0;i<n;i++){
	for(int j=n-i;j>0;j--)
	{
		System.out.print("*");
	}
	System.out.println();
	for(int k=n;k<n*2;k++)
	{
		for(int l=n;k<=k;k++)
		{
			System.out.print("*");
		}
	}
	System.out.println();
	
}
	}

}
