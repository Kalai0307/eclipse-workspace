import java.util.ArrayList;
import java.util.Collections;

public class AbstractDemo {

	public static void main(String[] args) {
	    String s="abac";
	    int n=10;
		ArrayList<Character> a= new ArrayList<>();
	      int j=s.length();
	    for(int i=0;i<n;)
	    
	    { 
	      for(int p=0;p<j;p++)
	    {
	      a.add(s.charAt(p));
	      
	    }
	     j=0; 
	    }
	    
	    int count=0;
	    for(int i :a)
	    {
	      if('a'==a.get(i))
	      {
	        count++;
	      }
	    }
	    System.out.println("count"+count);
	    
	    }
	

    
	}


