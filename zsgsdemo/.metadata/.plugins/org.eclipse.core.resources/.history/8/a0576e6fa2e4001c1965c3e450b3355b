package com.zoho.busreservation;

public class SeatReservation {

	public static void main(String[] args) {
		System.out.println(" BUS SEAT RESERVATION");
		System.out.println("PLEASE CHOOSE ONE SEAT");
		int n=5;
		 int p= '*';
		 int seat=1;
		for(int i=1;i<=6;i++)
		{
			for(int j=1;j<=6;j++)
			{
				if(j%4==0)
				{
					System.out.print("   ");
					seat--;
					
				}
				else  if(n==seat &&p!='k')
				{
					System.out.print('k');
				}
				else
				{
					System.out.print(seat+" ");
				}
			
				seat++;
					
			}
			System.out.println();
		}

	}

}
