import java.util.ArrayList;
import java.util.Collections;

public class AbstractDemo {

	public static void main(String[] args) {
	    String s="abac";
	    int n=10,couu=0;
		ArrayList<Character> a= new ArrayList<>();
	      int j=s.length();
	    for(int i=0;i<2;i++)
	    
	    { 
	      for(int p=0;p<j;p++)
	    {
	    	  couu++;
	      a.add(s.charAt(p));
	      
	      
	    }
	      if (couu==10)
	    	  break;
	     j=0; 
	    }
	    
	    int count=0;
	    for(int i :a)
	    {
	      if(a.get(i)=='a')
	      {
	        count++;
	      }
	    }
	    System.out.println("count"+count);
	    
	    }
	

    
	}


