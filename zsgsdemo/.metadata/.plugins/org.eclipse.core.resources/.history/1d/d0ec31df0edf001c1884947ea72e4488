package com.zoho.bus;

import java.util.ArrayList;

public class EqualizeTheArary {

	public static void main(String[] args) {
		ArrayList<Integer> c= new ArrayList<Integer>();
		c.add(3);
		c.add(3);
		c.add(2);
		c.add(1);
		c.add(3);
		//c.add(1);
	//	c.add(0);
		 Integer a[]= new Integer[c.size()];
	      a=c.toArray(a);
	      int count=0;
	      int removalcount=0;
	      for(int i=0;i<a.length;i++)
	    {
	      for(int j=0;j<a.length;j++)
	      {
	    	 // System.out.println(a[i]);
	        if(a[i]==a[j])
	        {
	          count++;
	          
	        }
	        
	      }
	      System.out.print( "cou "+count);
	      if(count==1)
	      {
	        removalcount++;
	      }
	      count=0;
	    }
	    int result= a.length-removalcount-1;// TODO Auto-generated method stub
System.out.println(result);
	}

}
