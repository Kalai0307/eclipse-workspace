package zsgsdemo;

public class LeftRotate_11 {

	public static void main(String[] args) {
		int a[]= {1,2,3,4,5};
		int n = a.length;
		System.out.println("numof ele"+n);
		int temp = 0;
		for(int i=0;i<n;i++)
		{
			temp =a[0];
			a[i]=a[i+1];
			
		}
		a[n-1]=temp;
		for(int i=0;i<n;i++)
		System.out.println(a[i]);
	}

}
