package zsgsdemo;
class Pattern
{
	public void print(String n[])
	{
		for(int i =1;i<=5;i++)
		{
			for(int j=1;j<=5;j++)
			{
				if(i==j||(i+j)==(5-1))
				{
					System.out.print(n[i]);
				}
				else
					System.out.print(" ");
			}
			System.out.println("\n");
		}
	}
}
public class Xpattern_7 {

	public static void main(String[] args) {
		
		String n=(String) "12345";
	Pattern obj=new Pattern();
	obj.print(n);
		

	}

}
