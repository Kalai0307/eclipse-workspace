package zsgsdemo;
class Pattern
{
	public void print(String n)
	{
		for(int i =1;i<=n.length();i++)
		{
			for(int j=1;j<=n.length();j++)
			{
				if(i==j||(i+j)==(n.length()))
				{
					System.out.print(j);
				}
				else
					System.out.print(" ");
			}
			System.out.println("\n");
		}
	}
}
public class Xpattern_7 {

	public static void main(String[] args) {
		
		String n="12345";
	Pattern obj=new Pattern();
	obj.print(n);
		

	}

}
